{
    "name": "game-worldmap-colorizer",
    "publisher": "santagergely90",
    "displayName": "Game Worldmap Colorizer",
    "description": "",
    "version": "0.13.0",
    "icon": "img/vscode_extension_icon.png",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "gameworldmap",
                "aliases": [
                    "Game World Map",
                    "gameworldmap"
                ],
                "extensions": [
                    ".gwm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gameworldmap",
                "scopeName": "source.gwm",
                "path": "./syntaxes/gamemap.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "Game World Map",
                "path": "./themes/gamemapTheme.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gsanta/game-worldmap-generator"
    }
}
