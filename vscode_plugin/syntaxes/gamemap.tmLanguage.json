{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Game World Map",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.map.gameworldmap",
					"begin": "map {",
					"end": "}",
					"patterns": [
						{
							"name": "keyword.W.gameworldmap",
							"match": "W"
						},
						{
							"name": "keyword.hash.gameworldmap",
							"match": "#"
						},
						{
							"name": "keyword.D.gameworldmap",
							"match": "D"
						},
						{
							"name": "keyword.I.gameworldmap",
							"match": "I"
						}
					]
				},
				{
					"name": "keyword.definitions.gameworldmap",
					"begin": "definitions {",
					"end": "}",
					"patterns": [
						{
							"name": "keyword.W.gameworldmap",
							"match": "W[\\s\\S]*"
						},
						{
							"name": "keyword.hash.gameworldmap",
							"match": "#[\\s\\S]*"
						},
						{
							"name": "keyword.D.gameworldmap",
							"match": "D[\\s\\S]*"
						},
						{
							"name": "keyword.I.gameworldmap",
							"match": "I[\\s\\S]*"
						}
					]
				}
			]
		}
	},
	"scopeName": "source.gwm"
}